{
  "name": "vue-demo",
  "version": "2.0.9",
  "private": true,
  "license": "MIT",
  "author": "Sunny Hirai <thesunny@gmail.com>",
  "files": [
    "README.md",
    ".dist/"
  ],
  "main": ".dist/index.cjs.js",
  "module": ".dist/index.esm.js",
  "types": ".dist/index.d.ts",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.2.1",
    "concurrently": "^7.2",
    "dotenv": "16.x",
    "eslint": "^8.4.1",
    "eslint-config-custom": "workspace:*",
    "prettier": "^2",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "tsconfig": "workspace:*",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.3.3",
    "vue": "^3"
  },
  "scripts": {
    "-- fix": "# fix syntax",
    "fix": "pnpm fix:prettier && pnpm fix:eslint",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "pnpm lint:prettier --write",
    "-- lint": "# lint syntax",
    "lint": "concurrently 'pnpm lint:prettier' 'pnpm lint:eslint' 'pnpm lint:tsc'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint:tsc": "tsc --build ./ --force",
    "-- next": "# NextJs",
    "open": "open -a 'google chrome' http://localhost:3734",
    "start:local": "vite --port 3734",
    "-- preset": "# Run preset from ~/presets",
    "preset": "ts-node --project $HOME/presets/tsconfig.ts-node.json $HOME/presets/bin/index.ts",
    "-- end": "# end"
  }
}
